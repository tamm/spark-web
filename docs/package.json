{
  "name": "@spark-web/docs",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --sequential build:*",
    "build:contentlayer": "INIT_CWD=$PWD yarn contentlayer build",
    "build:docs": "yarn generate-components-index && INIT_CWD=$PWD next build",
    "build:example-site": "yarn workspace example-site build",
    "build:playroom": "yarn generate-components-index && playroom build",
    "build:storybook": "build-storybook --force-build-preview --preview-url=/storybook/iframe.html -o ./public/storybook",
    "check:types": "yarn generate-components-index && yarn build:contentlayer && tsc --noEmit",
    "clean": "git clean -fXd .",
    "dev:docs": "yarn generate-components-index && INIT_CWD=$PWD next dev",
    "dev:playroom": "yarn generate-components-index && playroom start",
    "dev:storybook": "start-storybook -p 6006",
    "dev:storybook:no-cache": "yarn dev:storybook --no-manager-cache",
    "generate-components-index": "./scripts/generate-components-index.mjs",
    "lint": "next lint",
    "start": "next start"
  },
